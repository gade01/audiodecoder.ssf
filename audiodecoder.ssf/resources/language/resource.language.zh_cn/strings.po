msgctxt "#30000"
msgid "Default length"
msgstr "默认长度"

msgctxt "#30001"
msgid "Used if specified file does not provide the length."
msgstr "用于未提供长度的特定文件。"

msgctxt "#30002"
msgid "unused"
msgstr "未使用"

msgctxt "#30003"
msgid "{0:d} s"
msgstr "{0:d} 秒"

msgctxt "#30004"
msgid "Default fade out time"
msgstr "默认淡出时间"

msgctxt "#30005"
msgid "Used if specified file does not provide the length and fade."
msgstr "用于未提供长度和淡出的特定文件。"

